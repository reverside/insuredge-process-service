										<element name="typeOfLicense" type="string"/>
										<element name="dateOfLicense" type="string"/>
    		    					</sequence>
    		    				</complexType>
    		    			</element>
    		    			<element name="more">
    		    				<complexType>
    		    					<sequence>
    		    						<element name="parkingType" type="string"/>
    		    						<element name="coverType" type="string"/>
    		    					</sequence>
    		    				</complexType>
    		    			</element>
    		    			<element name="amount">
    		    				<complexType>
    		    					<sequence>
    		    						<element name="premium" type="string"/>
    		    						<element name="excess" type="string"/>
