							<element name="email" type="string"/>
							<element name="address" type="string"/>
    		    		</sequence>
    		    	</complexType>
    		    </element>
    		    <element name="product" minOccurs="1" maxOccurs="10">
    		    	<complexType>
    		    		<sequence>
